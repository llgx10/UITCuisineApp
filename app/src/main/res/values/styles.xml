<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="borderImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">24dp</item>
    </style>
    <style name="borderAvatar">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>
    <style name="borderAvatar2">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">24dp</item>
    </style>
    <style name="customRatingHeart" parent="Widget.AppCompat.RatingBar">
        <item name="android:progressDrawable">@drawable/custom_rating_heart</item>
        <item name="android:minHeight">15dip</item>
        <item name="android:maxHeight">47dip</item>
    </style>
    <style name="borderCategoryImage">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
    </style>
    <style name="DatePickerStyle" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar.MinWidth">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@color/myOrange</item>
        <item name="android:textSize">30sp</item>
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
    </style>



</resources>